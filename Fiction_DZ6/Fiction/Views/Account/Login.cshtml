@inject IHttpContextAccessor _contextAccessor@model AccountLoginViewModel@{     ViewData["Title"] = "Login";}<div class="text-center">    <h1 class="display-4">Login</h1>    <form asp-controller="Account" asp-action="Login" asp-route-returnUrl="@_contextAccessor.HttpContext.Request.Query["ReturnUrl"]" method="post">        <div asp-validation-summary="All"></div>        <label asp-for="Email"></label><br>        <input asp-for="Email" type="text" /><br>        <label asp-for="Password"></label><br>        <input asp-for="Password" type="password" /><br>        <label asp-for="RememberMe"></label>        <input asp-for="RememberMe" /><br>        <button type="submit">Login</button>    </form></div>