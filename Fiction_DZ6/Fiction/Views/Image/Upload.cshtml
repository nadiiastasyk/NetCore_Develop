@model ImageUploadViewModel@{ ViewData["Title"] = "Upload Image";}<div class="text-center">    <h1 class="display-4">Upload Image</h1>    @if (Model.UploadStage == UploadStage.Upload)    {<form enctype="multipart/form-data" asp-controller="Image" asp-action="Upload" method="post">    <input asp-for="Image" type="file">    <button type="submit">Upload Image</button></form> }else{<h3 class="display-4">Upload image completed.</h3>    }</div>