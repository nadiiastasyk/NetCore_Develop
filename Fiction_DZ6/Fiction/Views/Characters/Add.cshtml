@model Character@{     ViewData["Title"] = "Add Characters";}<div class="text-center">    <h1 class="display-4">Add Characters</h1>    <form asp-controller="Characters" asp-action="Add" method="post">        <div asp-validation-summary="All"></div>        <label asp-for="Name"></label><br>        <input asp-for="Name" type="text" /><br>        <span asp-validation-for="Name"></span>        <label asp-for="Age"></label><br>        <input asp-for="Age" type="number" /><br>        <span asp-validation-for="Age"></span>        <label asp-for="Gender"></label><br>        <select asp-for="Gender" asp-items="Html.GetEnumSelectList<Gender>()" type="text"></select><br>        <span asp-validation-for="Gender"></span>        <label asp-for="StoryId"></label><br>        <input asp-for="StoryId" type="number" /><br>        <span asp-validation-for="StoryId"></span>        <button type="submit">Add character</button>    </form></div>