@model AccountRegisterViewModel@{ ViewData["Title"] = "Register";}<div class="text-center">    <h1 class="display-4">Register</h1>    <form asp-controller="Account" asp-action="Register" method="post">        <div asp-validation-summary="All" class="text-danger text-sm-center align-middle"></div>        <label asp-for="Email"></label><br>        <input asp-for="Email" type="text" /><br>        <label asp-for="Password"></label><br>        <input asp-for="Password" type="password" /><br>        <label asp-for="ConfirmationPassword"></label><br>        <input asp-for="ConfirmationPassword" type="password" /><br>        <label asp-for="FirstName"></label><br>        <input asp-for="FirstName" type="text" /><br>        <label asp-for="LastName"></label><br>        <input asp-for="LastName" type="text" /><br>        <button type="submit">Register</button>    </form></div>